{
    "framework": "nextjs",
    "redirects": [
        {
            "source": "/:path*",
            "has": [
                {
                    "type": "host",
                    "value": "paing.xyz"
                }
            ],
            "destination": "https://store.paing.xyz/:path*",
            "permanent": true
        },
        {
            "source": "/:path*",
            "has": [
                {
                    "type": "host",
                    "value": "www.paing.xyz"
                }
            ],
            "destination": "https://store.paing.xyz/:path*",
            "permanent": true
        }
    ]
}